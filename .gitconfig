[push]
      default = simple

[color]
      ui = true

[color]
      diff = auto
      branch = auto
      status = auto

[alias]
      b = branch
      ca = commit -a
      co = checkout -b
      meld = !~/.bin/git/git-meld.pl
      lod = log --oneline --decorate
      lgraph = log --oneline --decorate --graph
      rem = remote add origin
